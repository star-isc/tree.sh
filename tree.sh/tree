#!/bin/sh

conf() {
  printf "
$(c 0 32)    #          $(c 0 34)  os$(c 0 37) $(Os)
$(c 0 32)   ###o        $(c 0 34)  os$(c 0 37) $(Os)
$(c 0 32) #o# \#,_#,#   $(c 0 34)  kr$(c 0 37) $(Kernel)
$(c 0 32)###$(c 0 34)\ |/$(c 0 32)#o#  $(c 0 32)  
$(c 0 32) # $(c 0 34){}{   $(c 0 32)#  $(c 0 36)  
    }{{        $(c 0 34)  
   ,' ',       $(c 0 34)  
  \n"
}
